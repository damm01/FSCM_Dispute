				<Text wrapping="false" text="{arg_string}" />                                                                                                                                                                                                              
			</cells>                                                                                                                                                                                                                                                    
		</ColumnListItem>                                                                                                                                                                                                                                            
</core:FragmentDefinition>                                                                                                                                                                                                                                     